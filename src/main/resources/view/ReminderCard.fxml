<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane xmlns="http://javafx.com/javafx/17"
            xmlns:fx="http://javafx.com/fxml/1"
            prefWidth="360.0" prefHeight="40.0">
    <children>
        <HBox spacing="8.0"
              AnchorPane.leftAnchor="8.0"
              AnchorPane.rightAnchor="8.0"
              AnchorPane.topAnchor="6.0"
              AnchorPane.bottomAnchor="6.0">
            <children>
                <Label fx:id="index" styleClass="cell_small_label"/>
                <Label fx:id="content" wrapText="true"/>
            </children>
        </HBox>
    </children>
</AnchorPane>
